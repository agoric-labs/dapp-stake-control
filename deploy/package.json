{
  "name": "deploy-stake-management",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "devDependencies": {
    "@agoric/deploy-script-support": "^0.10.4-u19.2",
    "agoric": "^0.22.0-u19.3",
    "esbuild": "^0.25.2",
    "execa": "^9.5.2"
  },
  "scripts": {
    "build": "esbuild ../contract/stake.contract.js --platform=node --format=esm --bundle --outfile=dist/stake.contract.bundle.js && agoric run src/init-contract.js",
    "test": "yarn build && ava",
    "deploy": "yarn build && runInsideContainer=true planFile=startStakeManagement-plan.json node deploy.js",
    "register:token": "runInsideContainer=true planFile=eval-register-interchain-bank-assets-plan.json node deploy.js"
  },
  "ava": {
    "extensions": {
      "js": true,
      "ts": "module"
    },
    "files": [
      "test/**/*.test.*"
    ],
    "nodeArguments": [
      "--loader=ts-blank-space/register",
      "--no-warnings"
    ],
    "require": [
      "@endo/init/debug.js"
    ],
    "timeout": "20m",
    "workerThreads": false
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "dependencies": {
    "@agoric/cosmic-swingset": "0.41.4-dev-0e6a9f4.0",
    "@agoric/ertp": "^0.16.3-u20.0",
    "@agoric/internal": "^0.4.0-u19.2",
    "@agoric/orchestration": "0.1.1-dev-9f21b9d.0",
    "@agoric/swingset-vat": "^0.33.0-u19.2",
    "@agoric/telemetry": "^0.6.3-u19.2",
    "@agoric/xsnap": "^0.14.3-u19.2",
    "@agoric/zoe": "^0.27.0-u19.2",
    "@endo/errors": "^1.2.10",
    "@endo/far": "^1.1.11",
    "ava": "^5.3.0",
    "node-fetch-cache": "^5.0.2"
  }
}
